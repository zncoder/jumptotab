{
  "name": "TabMenu",
  "version": "1.2.0",
  "manifest_version": 2,
  "description": "List tabs",
  "icons": {
    "48": "48.png"
  },
	"permissions": [ "tabs" ],
  "background": {
    "scripts": [ "background.js" ]
  },
	"browser_action": {
		"browser_style": true,
		"default_icon": "32.png",
		"default_title": "List tabs"
	}
}
